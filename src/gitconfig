[alias]
  s = status
  br = branch
  co = checkout
  c = commit
  d = diff
  po = !git push --set-upstream origin \"$(git rev-parse --abbrev-ref HEAD)\"
  amend = commit --amend --no-edit
  done = !git checkout $(git symbolic-ref refs/remotes/origin/HEAD | sed 's@^refs/remotes/origin/@@') && git pull && git branch -d @{-1}
[core]
	commitGraph = true
[diff]
	algorithm = patience
[gc]
	writeCommitGraph = true
[protocol]
	version = 2
[url "https://github.com/Shopify/"]
	insteadOf = git@github.com:Shopify/
	insteadOf = git@github.com:shopify/
	insteadOf = ssh://git@github.com/Shopify/
	insteadOf = ssh://git@github.com/shopify/
[user]
	name = Lucas Kim
	email = lucas.kim@shopify.com
[pager]
	branch = false
	grep = false
[credential]
	helper =
	helper = store --file /opt/dev/var/private/git_credential_store
[pull]
	rebase = false
